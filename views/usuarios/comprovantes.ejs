<% include ../partials/header%>
<div class="container">
    <div class="row">
        <div class="col-sm">
            <h3>Lista de comprovantes gerados</h3>
            <table class="table table-striped">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Comprovante</th>
                    <th scope="col">Data de Retirada</th>
                  </tr>
                </thead>
                <% usuario.comprovantes.reverse().forEach((i,idx,comp) => {%>
                <tbody>
                  <tr>
                    <th scope="row"></th>
                    <td><a href="/download/<%= i.comURL%>">Download</td>
                    <td><strong><%= moment(i.dataCriado).format('LLL');  %></strong></td>
                  </tr>
                </tbody>
                <%}); %>
              </table>
              
</div>
</div>

</div>


