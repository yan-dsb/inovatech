
<!DOCTYPE html>
<html lang="en">
    <head>
        <title>E-discard</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
        <link rel="stylesheet" href="stylesheets/main.css" type="text/css">
    </head>
    <body>
    <div>
        <h4>
        <% if(!currentuser){ %>
         <li><a href="/login"><span class="glyphicon glyphicon-log-in"  aria-hidden="true" ></span> Login</a></li>
         <li><a href="/register"><i class="material-icons md-18"></i> Sign Up</a></li>
        <% } else { %>
        <% currentuser.pessoas.forEach(pessoa => {%>
        <p><span class="glyphicon glyphicon-user icon-size"></span><%= pessoa.pesnome%></p>
        <%}); %>
        <%}%>

    </div>
    <div id="container" >

    <% if(errorArr && errorArr.length > 0){ %>
        <div class="alert alert-danger" role="alert"> <%= errorArr %> </div>
    <% } %>
    <% if(successArr && successArr.length > 0){ %>
        <div class="alert alert-success" role="alert"> <%= successArr %> </div>
    <% } %>
    <!--</div>-->
    </div>

    
